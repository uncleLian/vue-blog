{
    "name": "vue-blog",
    "version": "2.0.0",
    "private": true,
    "scripts": {
        "dev": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build:sit": "vue-cli-service build --mode sit",
        "build:report": "vue-cli-service build --report",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "driver.js": "^0.9.5",
        "element-ui": "^2.6.1",
        "file-saver": "^2.0.1",
        "js-cookie": "^2.2.0",
        "jszip": "^3.2.0",
        "mockjs": "^1.0.1-beta3",
        "normalize.css": "^8.0.1",
        "screenfull": "^4.0.1",
        "sortablejs": "^1.8.4",
        "tinymce": "^5.0.2",
        "tui-editor": "^1.3.2",
        "vue": "^2.6.8",
        "vue-backtop-pack": "^0.2.0",
        "vue-clipboard-pack": "^1.0.4",
        "vue-i18n": "^8.9.0",
        "vue-num-to": "^1.0.2",
        "vue-position-sticky": "^0.2.0",
        "vue-progressbar": "^0.7.5",
        "vue-router": "^3.0.2",
        "vuedraggable": "^2.20.0",
        "vuex": "^3.1.0",
        "xlsx": "^0.14.1"
    },
    "devDependencies": {
        "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0-beta.2",
        "@vue/babel-preset-jsx": "^1.0.0-beta.2",
        "@vue/cli-plugin-babel": "^3.5.0",
        "@vue/cli-plugin-eslint": "^3.5.0",
        "@vue/cli-service": "^3.5.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "script-loader": "^0.7.2",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "svg-sprite-loader": "^4.1.3",
        "vue-template-compiler": "^2.6.8"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/standard"
        ],
        "rules": {
            "no-undef": 0,
            "indent": 0,
            "camelcase": 0,
            "space-before-function-paren": 0,
            "vue/require-component-is": 0
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
